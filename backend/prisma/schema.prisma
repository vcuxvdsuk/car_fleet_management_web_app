
generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}
model Vehicle {
    id           String   @id @default(auto()) @map("_id") @db.ObjectId
    licensePlate String   @unique
    status       String
    createdAt    DateTime @default(now())
}
